<div class="container">

<h3 class="center pink-text text-lighten-3">Welcome Back!</h3>

  <%= form_tag '/sessions' do %>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
    <br><br>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <br><br>
    <%= submit_tag "Log In", class: "waves-effect waves-light btn pink lighten-3" %>
  <% end %>

</div>
